	background-color: rgba(51, 51, 51, 1);

	height: 30px;
	width: auto;
	
	align-items: stretch;
	align-self: stretch;

	flex-direction: row;
	flex-grow: 0;
	flex-shrink: 0;

	/* top - right - bottom - left */
	margin: 3px 5px 0px 0px;
