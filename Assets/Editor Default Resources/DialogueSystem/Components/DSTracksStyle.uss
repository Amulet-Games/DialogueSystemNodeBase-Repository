/*< -------------------- Option -------------------- >*/
.channel_Track_Port {

	height: auto;
	width: auto;

	align-content: flex-start;
	align-items: center;
	align-self: flex-start;
	
	flex-direction: row;
	flex-grow: 0;
	flex-shrink: 0;

	/* top - right - bottom - left */
	margin: 0px 0px 0px 0px;
	padding: 0px 4px 0px 4px;
	border-width: 0px 0px 0px 0px;
	border-radius: 0px 0px 0px 0px;
}

.channel_Track_Port
.channel_Track_Connector {

	background-color: rgba(255, 255, 255, 1);

	height: 14px;
	width: 14px;

	right: 1px;

	align-content: flex-start;
	align-items: center;
	align-self: auto;
	
	justify-content: center;

	flex-grow: 0;
	flex-shrink: 1;

	/* top - right - bottom - left */
	margin: 0px 0px 0px -9.5px;
	padding: 0px 4px 0px 4px;
	border-width: 0px 0px 0px 0px;
	border-radius: 16px 16px 16px 16px;
}

.channel_Track_Port
.channel_Track_Label {

	color: rgba(193, 193, 193, 1);

	font-size: 15px;

	height: 16px;
	width: auto;
	
	left: 2px;

	-unity-text-align: middle-left;
	white-space: nowrap;

	flex-grow: 0;
	flex-shrink: 0;
	flex-basis: auto;

	/* top - right - bottom - left */
	margin: 0px 4px 0px 4px;
	padding: 0px 0px 2px 0px;
	border-width: 0px 0px 0px 0px;
	border-radius: 0px 0px 0px 0px;
}

.channel_Track_Port_Connected
.channel_Track_Label {

	color: rgba(128, 209, 190, 1);

	font-size: 16px;

	-unity-font-style: bold-and-italic;

	/* top - right - bottom - left */
	padding: 0px 0px 0px 1px;
}

.channel_Track_Port
.channel_Track_Cap {

	background-color: rgba(56, 171, 170, 1);

	height: 10px;
	width: 10px;

	/* top - right - bottom - left */
	border-radius: 16px 16px 16px 16px;
}