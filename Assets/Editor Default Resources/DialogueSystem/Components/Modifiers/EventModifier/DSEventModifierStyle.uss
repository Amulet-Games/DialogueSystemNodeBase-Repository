/*< ---------------------------------------------------------- Helper Button Container >*/
.eventModifier-helperButton-container {

	align-items: center;

	flex-shrink: 0;
	flex-direction: row;

	justify-content: flex-end;
	
	/* top - right - bottom - left */
	margin: 0px 0px 2px 4px;
}


/*< ---------------------------------------------------------- Move Up Button >*/
.eventModifier-moveUp-button {

	align-self: center;

	height: 21px;
	width: 21px;

	flex-shrink: 0;

	/* top - right - bottom - left */
	margin: 3px 2px 0px 0px;
}

.eventModifier-moveUp-button:disabled {

	-unity-background-image-tint-color: rgba(255, 255, 255, 0.35);
}

.eventModifier-moveUp-button:enabled:hover
.background-highlighter {
	
	align-self: center;
	
	background-color: rgba(255, 255, 255, 0.133);

	height: 21px;
	width: 21px;

	/* top - right - bottom - left */
	border-radius: 4px;
}


/*< ---------------------------------------------------------- Move Down Button >*/
.eventModifier-moveDown-button {

	align-self: center;

	height: 21px;
	width: 21px;

	flex-shrink: 0;

	/* top - right - bottom - left */
	margin: 3px 0px 0px 0px;
}

.eventModifier-moveDown-button:disabled {

	-unity-background-image-tint-color: rgba(255, 255, 255, 0.35);
}

.eventModifier-moveDown-button:enabled:hover
.background-highlighter {
	
	align-self: center;
	
	background-color: rgba(255, 255, 255, 0.133);

	height: 21px;
	width: 21px;

	/* top - right - bottom - left */
	border-radius: 4px;
}


/*< ---------------------------------------------------------- Rename Button >*/
.eventModifier-rename-button {

	align-self: center;

	height: 24px;
	width: 24px;

	flex-shrink: 0;

	/* top - right - bottom - left */
	margin: 2.5px -2px 0px 0px;
}

.eventModifier-rename-button:disabled {
	
	-unity-background-image-tint-color: rgba(255, 255, 255, 0.35);
}

.eventModifier-rename-button:enabled:hover
.background-highlighter {
	
	align-self: center;
	
	background-color: rgba(255, 255, 255, 0.133);

	height: 21px;
	width: 21px;

	/* top - right - bottom - left */
	margin: 1px 0px 0px 0px;
	border-radius: 4px;
}


/*< ---------------------------------------------------------- Remove Button >*/
.eventModifier-remove-button {

	align-self: center;

	height: 27px;
	width: 26.5px;

	flex-shrink: 0;

	/* top - right - bottom - left */
	margin: 4px 2.5px 0px 0px;
}

.eventModifier-remove-button:disabled {

	-unity-background-image-tint-color: rgba(255, 255, 255, 0.35);
}

.eventModifier-remove-button:enabled:hover
.background-highlighter {
	
	align-self: center;
	
	background-color: rgba(255, 255, 255, 0.133);
	
	height: 21px;
	width: 21px;

	/* top - right - bottom - left */
	margin: 2.5px 0px 0px 0px;
	border-radius: 4px;
}


/*< ---------------------------------------------------------- Dialogue Event Container >*/
.eventModifier-dialogueEvent-container {
	
	background-color: rgba(42, 42, 42, 1);

	/* top - right - bottom - left */
	margin: 0px 6px 0px 6px;
	border-radius: 4px;
}


/*< ---------------------------------------------------------- Dialogue Event Label >*/
.eventModifier-dialogueEvent-label {
	
	color: rgba(210, 210, 210, 1);
	
	flex-shrink: 0;

	-unity-font-definition: url('project:/Assets/Data/Fonts/Inter/asset/Inter-Regular-Static-SDF.asset');
	
	white-space: nowrap;
	
	word-spacing: 9px;

	/* top - right - bottom - left */
	margin: 3px 0px 0px 6px;
}


/*< ---------------------------------------------------------- Dialogue Event Field >*/
.eventModifier-dialogueEvent-field {

	height: 27px;
	width: Auto;

	align-self: center;

	flex-grow: 1;
	flex-direction: row;

	overflow: hidden;

	-unity-overflow-clip-box: content-box;

	/* top - right - bottom - left */
	margin: 3px 6px 6px 6px;
	border-radius: 4px;
}

.eventModifier-dialogueEvent-field:hover {

	border-color: rgba(71, 71, 71, 1);

	/* top - right - bottom - left */
	border-width: 1px;
}

.eventModifier-dialogueEvent-field:focus {

	border-color: rgba(58, 121, 187, 1);

	/* top - right - bottom - left */
	border-width: 1px;
}

.eventModifier-dialogueEvent-field
.object-field-input {

	background-color: rgba(32, 32, 32, 1);
	border-color: rgba(32, 32, 32, 1);

	flex-basis: 0;
	flex-grow: 1;
	flex-shrink: 0;
	flex-direction: row;

	overflow: hidden;

	/* top - right - bottom - left */
	border-radius: 4px;
}

.eventModifier-dialogueEvent-field:hover
.object-field-input,

.eventModifier-dialogueEvent-field:focus
.object-field-input {

	/* top - right - bottom - left */
	margin: -1px -1.5px -1px -1.5px;
}

.eventModifier-dialogueEvent-field
.object-field-display {
	
	flex-basis: 0;
	flex-grow: 1;
	flex-shrink: 0;
	flex-direction: row;

	-unity-slice-top: 4;
	-unity-slice-right: 4;
	-unity-slice-bottom: 4;
	-unity-slice-left: 4;
}

.eventModifier-dialogueEvent-field
.object-field-display-image {

	height: 18.5px;
	width: 17px;

	align-self: center;

	/* top - right - bottom - left */
	margin: 0px 0px 0.5px 4px;

	--unity-image-tint-color: white;
}

.eventModifier-dialogueEvent-field.object-field-empty
.object-field-display-image {

	--unity-image-tint-color: rgba(81, 81, 81, 1);
}

.eventModifier-dialogueEvent-field
.object-field-display-label {

	color: white;

	align-self: Center;

	font-size: 13px;

	flex-basis: 0;
	flex-grow: 1;
	flex-shrink: 0;

	overflow: hidden;

	white-space: nowrap;
	
	-unity-font-definition: url('project:/Assets/Data/Fonts/Inter/asset/Inter-Medium-Static-SDF.asset');
	
	/* top - right - bottom - left */
	margin: 0px 3px 0px 3px;
}

.eventModifier-dialogueEvent-field.object-field-empty
.object-field-display-label {
	
	color: rgba(81, 81, 81, 1);
}

.eventModifier-dialogueEvent-field
.unity-object-field__selector {

	background-color: rgba(0, 0, 0, 0);

	height: 18px;
	width: 20px;

	align-self: Center;

	flex-shrink: 0;
	
	-unity-slice-top: 0;
	-unity-slice-right: 0;
	-unity-slice-bottom: 0;
	-unity-slice-left: 0;
	-unity-slice-scale: 0;

	/* top - right - bottom - left */
	margin: 0px 1px 0px 0px;
}

.eventModifier-dialogueEvent-field
.unity-object-field__selector:hover
.background-highlighter {
	
	align-self: center;
	
	background-color: rgba(255, 255, 255, 0.133);

	height: 16.5px;
	width: 16.5px;
	
	/* top - right - bottom - left */
	margin: 0.5px 0px 0px 0px;
	border-radius: 4px;
}


/*< ---------------------------------------------------------- Start Delay Container >*/
.eventModifier-startDelay-container {
	
	background-color: rgba(42, 42, 42, 1);

	width: 135px;

	/* top - right - bottom - left */
	margin: 6px;
	border-radius: 4px;
}


/*< ---------------------------------------------------------- Start Delay Label >*/
.eventModifier-startDelay-label {
	
	color: rgba(210, 210, 210, 1);
	
	flex-shrink: 0;

	-unity-font-definition: url('project:/Assets/Data/Fonts/Inter/asset/Inter-Regular-Static-SDF.asset');
	
	white-space: nowrap;
	
	word-spacing: 3px;

	/* top - right - bottom - left */
	margin: 2.5px 0px 0px 6px;
}


/*< ---------------------------------------------------------- Delay Seconds Container >*/
.eventModifier-delaySeconds-container {
	
	flex-direction: row;

	justify-content: space-between;
}


/*< ---------------------------------------------------------- Delay Seconds Field >*/
.eventModifier-delaySeconds-field {
	
	background-color: rgba(32, 32, 32, 1);

	height: 27px;
	width: 63px;

	align-self: center;

	flex-shrink: 0;
	flex-direction: row;

	overflow: hidden;

	white-space: nowrap;

	-unity-overflow-clip-box: content-box;

	/* top - right - bottom - left */
	margin: 4px 6px 6px 6px;
	border-radius: 4px;

	--unity-selection-color: rgba(61, 128, 223, 0.65);
	--unity-cursor-color: rgba(180, 180, 180, 1);
}

.eventModifier-delaySeconds-field:hover {
	
	border-color: rgba(101, 101, 101, 1);

	/* top - right - bottom - left */
	border-width: 0.5px;
}

.eventModifier-delaySeconds-field:focus {
	
	border-color: rgba(58, 121, 187, 1);

	/* top - right - bottom - left */
	border-width: 0.5px;
}

.eventModifier-delaySeconds-field
.double-field-input {

	cursor: text;
	
	flex-basis: auto;
	flex-grow: 1;
	flex-direction: row;

	overflow: scroll;
	
	-unity-overflow-clip-box: content-box;
	-unity-text-align: middle-left;

	--unity-sync-text-editor: true;
}

.eventModifier-delaySeconds-field
.double-field-element {

	color: white;
	cursor: text;

	font-size: 13px;
	
	flex-grow: 1;
	flex-shrink: 0;

	-unity-font-definition: url('project:/Assets/Data/Fonts/Inter/asset/Inter-Medium-Dynamic-SDF.asset');

	align-self: center;

	overflow: hidden;

	/* top - right - bottom - left */
	margin: 0px 4.5px 0px 4.5px;
}

.eventModifier-delaySeconds-field:hover
.double-field-element,

.eventModifier-delaySeconds-field:focus
.double-field-element
{
	/* top - right - bottom - left */
	margin: 0px 0px 0px 4px;
}


/*< ---------------------------------------------------------- Delay Seconds Label >*/
.eventModifier-delaySeconds-label {
	
	align-self: center;
	
	color: rgba(101, 101, 101, 1);

	font-size: 13px;
	
	flex-shrink: 0;

	-unity-font-definition: url('project:/Assets/Data/Fonts/Inter/asset/Inter-medium-Static-SDF.asset');
	
	white-space: nowrap;

	/* top - right - bottom - left */
	margin: 0px 6px 1px 0px;
}