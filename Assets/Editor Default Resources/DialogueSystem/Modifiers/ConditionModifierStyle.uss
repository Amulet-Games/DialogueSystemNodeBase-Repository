/*< ---------------------------------------------------------- Second Reflectable Object Field >*/
.conditionModifier-secondReflectableObject-field {

	height: 27px;
	width: Auto;

	align-self: center;

	flex-grow: 1;
	flex-direction: row;

	overflow: hidden;

	-unity-overflow-clip-box: content-box;

	/* top - right - bottom - left */
	margin: 3px 6px 3px 6px;
	border-radius: 4px;
}

.conditionModifier-secondReflectableObject-field:hover {

	border-color: rgba(71, 71, 71, 1);

	/* top - right - bottom - left */
	border-width: 1px;
}

.conditionModifier-secondReflectableObject-field:focus {

	border-color: rgba(58, 121, 187, 1);

	/* top - right - bottom - left */
	border-width: 1px;
}

.conditionModifier-secondReflectableObject-field
.object-field-input {

	background-color: rgba(32, 32, 32, 1);
	border-color: rgba(32, 32, 32, 1);

	flex-basis: 0;
	flex-grow: 1;
	flex-shrink: 0;
	flex-direction: row;

	overflow: hidden;

	/* top - right - bottom - left */
	border-radius: 4px;
}

.conditionModifier-secondReflectableObject-field:hover
.object-field-input,

.conditionModifier-secondReflectableObject-field:focus
.object-field-input {

	/* top - right - bottom - left */
	margin: -1px -1.5px -1px -1.5px;
}

.conditionModifier-secondReflectableObject-field
.object-field-display {
	
	flex-basis: 0;
	flex-grow: 1;
	flex-shrink: 0;
	flex-direction: row;

	-unity-slice-top: 4;
	-unity-slice-right: 4;
	-unity-slice-bottom: 4;
	-unity-slice-left: 4;
}

.conditionModifier-secondReflectableObject-field
